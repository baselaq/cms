#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/itsbaselaq/Codelab/cms/node_modules/.pnpm/typeorm@0.3.27_ioredis@5.8.2_pg@8.16.3_redis@4.7.1_reflect-metadata@0.2.2_ts-node@10.9._7f8861470d5d4e02702a0c0855568dac/node_modules/typeorm/node_modules:/Users/itsbaselaq/Codelab/cms/node_modules/.pnpm/typeorm@0.3.27_ioredis@5.8.2_pg@8.16.3_redis@4.7.1_reflect-metadata@0.2.2_ts-node@10.9._7f8861470d5d4e02702a0c0855568dac/node_modules:/Users/itsbaselaq/Codelab/cms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/itsbaselaq/Codelab/cms/node_modules/.pnpm/typeorm@0.3.27_ioredis@5.8.2_pg@8.16.3_redis@4.7.1_reflect-metadata@0.2.2_ts-node@10.9._7f8861470d5d4e02702a0c0855568dac/node_modules/typeorm/node_modules:/Users/itsbaselaq/Codelab/cms/node_modules/.pnpm/typeorm@0.3.27_ioredis@5.8.2_pg@8.16.3_redis@4.7.1_reflect-metadata@0.2.2_ts-node@10.9._7f8861470d5d4e02702a0c0855568dac/node_modules:/Users/itsbaselaq/Codelab/cms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
